{
    "Kind": {
        "Name": "Job"
    },
    "Spec": {
        "Name": "backup",
        "ResticPath": "/bin/false",

        "User": "Apache",
        "Service": "restic-repo1",

        "CheckPrecondsEveryMilli": 2000,
        "CheckPrecondsMaxTimes": 100,
        "Preconditions": {
            "HostsMustRoute": ["localhost"],
            "PathesMust": ["./testrepo"],
            "HostsMustConnect": [{"Host": "google.com", "Port": 80}]
    }
}