{
    "Kind": {
        "Name": "Job"
    },
    "Spec": {
        "Name": "forget",
        "ResticPath": "$HOME/.local/bin/restic",
        "Resticarguments": ["-r", "./testrepo", "forget", "--keep-last", "2"],

        "Username": "Apache",
        "Service": "restic-repo1",

        "CheckPrecondsEveryMilli": 2000,
        "CheckPrecondsMaxTimes": 100,
        "Preconditions": {
            "HostsMustRoute": ["localhost"],
            "PathesMust": ["./testrepo"],
            "HostsMustConnect": [{"Host": "google.com", "Port": 80}]
        }
    }
}